ano_atual = int(input("Digite o ano atual "))
ano_nascimento = int(input("Digite o ano de seu aniversario "))
anos= ano_atual - ano_nascimento
meses= anos*12
semanas= anos * 52.142857143
dias= anos*365
print("anos = ",anos)
print("meses = ",meses)
print("semanas = ",semanas)
print("dias = ",dias)
